<Window x:Class="PCLParaphernalia.ToolSymbolSetGenerate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WindowSymbolSetGenerate"
        Width="760"
        Height="780">
    <Grid>
        <Label Name="lbTitle"
               Width="270"
               Height="35"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Content="PCL Symbol Set Generate utility:"
               FontSize="18" />

        <GroupBox Name="grpDonorSymSetDetails"
                  Width="510"
                  Height="140"
                  Margin="0,35,0,0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  BorderThickness="5"
                  Header="Donor symbol set:">
            <Grid>
                <GroupBox Name="grpDonorSymSetGroup"
                          Width="90"
                          Height="70"
                          Margin="5,5,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          BorderThickness="2"
                          Header="Group:">
                    <Grid>
                        <RadioButton Name="rbDonorSymSetPreset"
                                     Grid.ColumnSpan="2"
                                     Height="16"
                                     Margin="0,5,0,0"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Click="rbDonorSymSetPreset_Click"
                                     Content="Preset"
                                     GroupName="DonorSymSetType"
                                     IsChecked="True" />
                        <RadioButton Name="rbDonorSymSetUserSet"
                                     Grid.ColumnSpan="2"
                                     Height="16"
                                     Margin="0,25,0,0"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Click="rbDonorSymSetUserSet_Click"
                                     Content="User set"
                                     GroupName="DonorSymSetType" />
                    </Grid>
                </GroupBox>

                <GroupBox Name="grpDonorSymSet"
                          Width="385"
                          Height="55"
                          Margin="100,5,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          BorderThickness="2"
                          Header="Character Set:">
                    <Grid>
                        <ComboBox Name="cbDonorSymSet"
                                  Width="175"
                                  Height="23"
                                  Margin="15,5,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top"
                                  IsEnabled="True"
                                  SelectionChanged="cbDonorSymSet_SelectionChanged" />
                        <Label Name="lbDonorSymSetId"
                               Width="25"
                               Height="25"
                               Margin="190,2,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="ID:" />
                        <TextBox Name="txtDonorSymSetIdNum"
                                 Width="40"
                                 Height="20"
                                 Margin="215,5,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 HorizontalContentAlignment="Right"
                                 VerticalContentAlignment="Bottom"
                                 FontFamily="Lucida Console"
                                 FontSize="12"
                                 IsEnabled="False" />
                        <TextBox Name="txtDonorSymSetIdAlpha"
                                 Width="20"
                                 Height="20"
                                 Margin="260,5,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 VerticalContentAlignment="Bottom"
                                 FontFamily="Lucida Console"
                                 FontSize="12"
                                 IsEnabled="False" />
                        <Label Name="lbDonorSymSetNo"
                               Width="45"
                               Height="25"
                               Margin="282,2,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Kind1:" />
                        <TextBox Name="txtDonorSymSetNo"
                                 Width="45"
                                 Height="20"
                                 Margin="325,5,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 HorizontalContentAlignment="Right"
                                 VerticalContentAlignment="Bottom"
                                 FontFamily="Lucida Console"
                                 FontSize="12"
                                 IsEnabled="False" />
                    </Grid>
                </GroupBox>

                <GroupBox Name="grpDonorSymSetMapType"
                          Width="210"
                          Height="45"
                          Margin="100,65,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          Header="Mapping">
                    <Grid>
                        <RadioButton Name="rbDonorSymSetMapStd"
                                     Height="16"
                                     Margin="10,5,0,0"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Click="rbDonorSymSetMapStd_Click"
                                     Content="Strict"
                                     GroupName="DonorSymSetMap"
                                     IsChecked="True" />
                        <RadioButton Name="rbDonorSymSetMapPCL"
                                     Height="16"
                                     Margin="80,5,0,0"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Click="rbDonorSymSetMapPCL_Click"
                                     Content="LaserJet"
                                     GroupName="DonorSymSetMap" />
                    </Grid>
                </GroupBox>

                <GroupBox Name="grpDonorSymSetFile"
                          Width="385"
                          Height="55"
                          Margin="100,55,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          BorderThickness="2"
                          Header="User-defined Symbol Set file:"
                          Visibility="Hidden">
                    <Grid>
                        <TextBox Name="txtDonorSymSetFile"
                                 Width="300"
                                 Height="20"
                                 Margin="0,5,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 VerticalContentAlignment="Bottom"
                                 FontFamily="Lucida Console"
                                 FontSize="12"
                                 LostFocus="txtDonorSymSetFile_LostFocus" />
                        <Button Name="btnDonorSymSetFileBrowse"
                                Width="60"
                                Height="20"
                                Margin="310,5,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Click="btnDonorSymSetFileBrowse_Click"
                                Content="browse ..."
                                Style="{StaticResource SecondaryButton}" />
                    </Grid>
                </GroupBox>
            </Grid>
        </GroupBox>

        <Button Name="btnDefineSymSet"
                Width="200"
                Height="40"
                Margin="515,50,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="btnDefineSymSet_Click"
                Content="Define symbol set"
                Style="{StaticResource PrimaryButton}" />
        <Button Name="btnGenerateSymSet"
                Width="200"
                Height="40"
                Margin="515,95,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="btnGenerateSymSet_Click"
                Content="Generate symbol set"
                Style="{StaticResource PrimaryButton}" />
        <Button Name="btnLogSave"
                Width="200"
                Height="40"
                Margin="515,140,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="btnLogSave_Click"
                Content="Save log"
                Style="{StaticResource PrimaryButton}" />

        <GroupBox Name="grpTargetSymSetDetails"
                  Width="750"
                  Height="575"
                  Margin="0,180,0,0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  BorderThickness="5"
                  Header="Target symbol set details:">
            <Grid>
                <GroupBox Name="grpTargetSymSet"
                          Width="205"
                          Height="50"
                          Margin="5,0,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          BorderThickness="2"
                          Header="Identifier:">
                    <Grid>
                        <Label Name="lbTargetSymSetId"
                               Width="25"
                               Height="25"
                               Margin="5,2,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="ID:" />
                        <TextBox Name="txtTargetSymSetIdNum"
                                 Width="40"
                                 Height="20"
                                 Margin="30,5,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 HorizontalContentAlignment="Right"
                                 VerticalContentAlignment="Bottom"
                                 FontFamily="Lucida Console"
                                 FontSize="12"
                                 GotFocus="txtTargetSymSetIdNum_GotFocus"
                                 LostFocus="txtTargetSymSetIdNum_LostFocus"
                                 TextChanged="txtTargetSymSetIdNum_TextChanged" />
                        <TextBox Name="txtTargetSymSetIdAlpha"
                                 Width="20"
                                 Height="20"
                                 Margin="75,5,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 VerticalContentAlignment="Bottom"
                                 FontFamily="Lucida Console"
                                 FontSize="12"
                                 GotFocus="txtTargetSymSetIdAlpha_GotFocus"
                                 LostFocus="txtTargetSymSetIdAlpha_LostFocus"
                                 TextChanged="txtTargetSymSetIdAlpha_TextChanged" />
                        <Label Name="lbTargetSymSetNo"
                               Width="45"
                               Height="25"
                               Margin="100,2,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Kind1:" />
                        <TextBox Name="txtTargetSymSetNo"
                                 Width="45"
                                 Height="20"
                                 Margin="140,5,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 HorizontalContentAlignment="Right"
                                 VerticalContentAlignment="Bottom"
                                 FontFamily="Lucida Console"
                                 FontSize="12"
                                 IsEnabled="False" />
                    </Grid>
                </GroupBox>

                <GroupBox Name="grpSymSetType"
                          Width="125"
                          Height="50"
                          Margin="215,0,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          BorderThickness="2"
                          Header="Type:">
                    <Grid>
                        <RadioButton Name="rbTypeMonoByte"
                                     Grid.ColumnSpan="2"
                                     Height="16"
                                     Margin="0,5,0,0"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Click="rbTypeMonoByte_Click"
                                     Content="7/8-bit"
                                     GroupName="IndexSymSetType"
                                     IsChecked="True" />
                        <RadioButton Name="rbTypeMultiByte"
                                     Grid.ColumnSpan="2"
                                     Height="16"
                                     Margin="60,5,0,0"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Click="rbTypeMultiByte_Click"
                                     Content="16-bit"
                                     GroupName="IndexSymSetType" />
                    </Grid>
                </GroupBox>

                <GroupBox Name="grpTargetCharReq"
                          Width="390"
                          Height="60"
                          Margin="340,0,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          BorderThickness="2"
                          Header="Character requirements:">
                    <Grid>
                        <TextBlock Name="tblkCharColls"
                                   Width="120"
                                   Height="20"
                                   Margin="7,-15,0,0"
                                   HorizontalAlignment="Left"
                                   Text="0x array value" />
                        <RadioButton Name="rbCharReqAll"
                                     Height="16"
                                     Margin="5,20,0,0"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Click="rbCharReqAll_Click"
                                     Content="All"
                                     GroupName="CharReq"
                                     IsChecked="True" />
                        <RadioButton Name="rbCharReqSpecific"
                                     Height="16"
                                     Margin="55,20,0,0"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Click="rbCharReqSpecific_Click"
                                     Content="Specific"
                                     GroupName="CharReq" />

                        <StackPanel Margin="1">
                            <ComboBox Name="cbCharColls"
                                      Width="250"
                                      Margin="125,2,0,0"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Top"
                                      ItemsSource="{Binding charColls}"
                                      SelectionChanged="cbCharColls_SelectionChanged">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <CheckBox Content="{Binding Desc}"
                                                  IsChecked="{Binding IsChecked,
                                                                      Mode=TwoWay}"
                                                  IsEnabled="{Binding IsEnabled}" />
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                            <TextBlock Name="tblkCharCollsText"
                                       Width="Auto"
                                       Height="20"
                                       Margin="155,-20,0,0"
                                       HorizontalAlignment="Left"
                                       Text="Select required collection(s):" />
                        </StackPanel>
                    </Grid>
                </GroupBox>

                <GroupBox Name="grpTargetSymSetFile"
                          Width="725"
                          Height="50"
                          Margin="5,50,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          BorderThickness="2"
                          Header="Target file:">
                    <Grid>
                        <TextBox Name="txtTargetSymSetFile"
                                 Width="590"
                                 Height="20"
                                 Margin="5,5,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 VerticalContentAlignment="Bottom"
                                 FontFamily="Lucida Console"
                                 FontSize="12"
                                 LostFocus="txtTargetSymSetFile_LostFocus" />
                        <Button Name="btnTargetSymSetFileBrowse"
                                Width="60"
                                Height="20"
                                Margin="630,5,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Click="btnTargetSymSetFileBrowse_Click"
                                Content="browse ..."
                                Style="{StaticResource SecondaryButton}" />
                    </Grid>
                </GroupBox>

                <GroupBox Name="grpMapping"
                          Width="725"
                          Height="448"
                          Margin="5,100,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top"
                          BorderThickness="2"
                          Header="Mapping to symbol indices:">
                    <Grid>
                        <GroupBox Name="grpIndexType"
                                  Width="85"
                                  Height="65"
                                  Margin="5,0,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top"
                                  BorderThickness="2"
                                  Header="Indexing:">
                            <Grid>
                                <RadioButton Name="rbIndexUnicode"
                                             Grid.ColumnSpan="2"
                                             Height="16"
                                             Margin="5,5,0,0"
                                             HorizontalAlignment="Left"
                                             VerticalAlignment="Top"
                                             Click="rbIndexUnicode_Click"
                                             Content="Unicode"
                                             GroupName="IndexType"
                                             IsChecked="True" />
                                <RadioButton Name="rbIndexMSL"
                                             Grid.ColumnSpan="2"
                                             Height="16"
                                             Margin="5,25,0,0"
                                             HorizontalAlignment="Left"
                                             VerticalAlignment="Top"
                                             Click="rbIndexMSL_Click"
                                             Content="MSL"
                                             GroupName="IndexType" />
                            </Grid>
                        </GroupBox>

                        <GroupBox Name="grpFormat"
                                  Width="85"
                                  Height="65"
                                  Margin="100,0,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top"
                                  BorderThickness="2"
                                  Header="Display:">
                            <Grid>
                                <RadioButton Name="rbMapDec"
                                             Grid.ColumnSpan="2"
                                             Height="16"
                                             Margin="5,5,0,0"
                                             HorizontalAlignment="Left"
                                             VerticalAlignment="Top"
                                             Click="rbMapDec_Click"
                                             Content="Decimal"
                                             GroupName="MapFormat"
                                             IsChecked="True" />
                                <RadioButton Name="rbMapHex"
                                             Grid.ColumnSpan="2"
                                             Height="16"
                                             Margin="5,25,0,0"
                                             HorizontalAlignment="Left"
                                             VerticalAlignment="Top"
                                             Click="rbMapHex_Click"
                                             Content="Hex"
                                             GroupName="MapFormat" />
                            </Grid>
                        </GroupBox>

                        <GroupBox Name="grpCodeMinMax"
                                  Width="200"
                                  Height="65"
                                  Margin="195,0,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top"
                                  BorderThickness="2"
                                  Header="Code-points:">
                            <Grid>
                                <Label Name="lbCodeMin"
                                       Width="35"
                                       Height="25"
                                       Margin="5,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="First:" />
                                <Label Name="lbCodeMax"
                                       Width="35"
                                       Height="25"
                                       Margin="5,20,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="Last:" />
                                <Label Name="lbCodeCt"
                                       Width="45"
                                       Height="25"
                                       Margin="85,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="Count:" />
                                <TextBox Name="txtCodeMin"
                                         Width="45"
                                         Height="20"
                                         Margin="40,1,0,0"
                                         HorizontalAlignment="Left"
                                         VerticalAlignment="Top"
                                         HorizontalContentAlignment="Right"
                                         VerticalContentAlignment="Bottom"
                                         FontFamily="Lucida Console"
                                         FontSize="12"
                                         IsEnabled="False" />
                                <TextBox Name="txtCodeMax"
                                         Width="45"
                                         Height="20"
                                         Margin="40,21,0,0"
                                         HorizontalAlignment="Left"
                                         VerticalAlignment="Top"
                                         HorizontalContentAlignment="Right"
                                         VerticalContentAlignment="Bottom"
                                         FontFamily="Lucida Console"
                                         FontSize="12"
                                         IsEnabled="False" />
                                <TextBox Name="txtCodeCt"
                                         Width="45"
                                         Height="20"
                                         Margin="130,1,0,0"
                                         HorizontalAlignment="Left"
                                         VerticalAlignment="Top"
                                         HorizontalContentAlignment="Right"
                                         VerticalContentAlignment="Bottom"
                                         FontFamily="Lucida Console"
                                         FontSize="12"
                                         IsEnabled="False" />
                                <Button Name="btnCodePtClear"
                                        Width="40"
                                        Height="20"
                                        Margin="85,20,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Click="btnCodePtClear_Click"
                                        Content="clear"
                                        Style="{StaticResource SecondaryButton}" />
                                <Button Name="btnCodePtClearAll"
                                        Width="50"
                                        Height="20"
                                        Margin="130,20,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Click="btnCodePtClearAll_Click"
                                        Content="clear all"
                                        Style="{StaticResource SecondaryButton}" />
                            </Grid>
                        </GroupBox>

                        <GroupBox Name="grpCodeOffset"
                                  Width="150"
                                  Height="65"
                                  Margin="405,0,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top"
                                  BorderThickness="2"
                                  Header="Offset:">
                            <Grid>
                                <Label Name="lbOffset"
                                       Width="25"
                                       Height="25"
                                       Margin="5,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="0x" />
                                <Label Name="lbRange"
                                       Width="40"
                                       Height="25"
                                       Margin="5,20,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="range:" />
                                <ComboBox Name="cbOffsetRange"
                                          Width="60"
                                          Height="20"
                                          Margin="50,1,0,0"
                                          HorizontalAlignment="Left"
                                          VerticalAlignment="Top"
                                          IsEnabled="True"
                                          SelectionChanged="cbOffsetRange_SelectionChanged" />
                                <TextBox Name="txtOffsetRangeMin"
                                         Width="40"
                                         Height="20"
                                         Margin="50,21,0,0"
                                         HorizontalAlignment="Left"
                                         VerticalAlignment="Top"
                                         HorizontalContentAlignment="Right"
                                         VerticalContentAlignment="Bottom"
                                         FontFamily="Lucida Console"
                                         FontSize="12"
                                         IsEnabled="False" />
                                <TextBox Name="txtOffsetRangeMax"
                                         Width="40"
                                         Height="20"
                                         Margin="95,21,0,0"
                                         HorizontalAlignment="Left"
                                         VerticalAlignment="Top"
                                         HorizontalContentAlignment="Right"
                                         VerticalContentAlignment="Bottom"
                                         FontFamily="Lucida Console"
                                         FontSize="12"
                                         IsEnabled="False" />
                            </Grid>
                        </GroupBox>

                        <GroupBox Name="grpC0C1"
                                  Width="135"
                                  Height="65"
                                  Margin="575,0,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top"
                                  BorderThickness="2"
                                  Header="Control-code ranges:">
                            <Grid>
                                <CheckBox Name="chkIgnoreC0"
                                          Margin="5,5,0,0"
                                          HorizontalAlignment="Left"
                                          VerticalAlignment="Top"
                                          Checked="chkIgnoreC0_Checked"
                                          Content="ignore C0 range"
                                          Unchecked="chkIgnoreC0_Unchecked" />
                                <CheckBox Name="chkIgnoreC1"
                                          Margin="5,25,0,0"
                                          HorizontalAlignment="Left"
                                          VerticalAlignment="Top"
                                          Checked="chkIgnoreC1_Checked"
                                          Content="ignore C1 range"
                                          Unchecked="chkIgnoreC1_Unchecked" />
                            </Grid>
                        </GroupBox>

                        <GroupBox Name="grpMap"
                                  Width="710"
                                  Height="370"
                                  Margin="5,55,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top"
                                  BorderThickness="2"
                                  Header="">
                            <Grid>
                                <TextBox Name="txtMapOffset"
                                         Width="55"
                                         Height="20"
                                         Margin="0,1,0,0"
                                         HorizontalAlignment="Left"
                                         VerticalAlignment="Top"
                                         HorizontalContentAlignment="Left"
                                         VerticalContentAlignment="Bottom"
                                         FontFamily="Lucida Console"
                                         FontSize="12"
                                         IsEnabled="False" />

                                <Label Name="lbMap0x_0"
                                       Width="40"
                                       Height="25"
                                       Margin="50,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__0" />
                                <Label Name="lbMap0x_1"
                                       Width="40"
                                       Height="25"
                                       Margin="91,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__1" />
                                <Label Name="lbMap0x_2"
                                       Width="40"
                                       Height="25"
                                       Margin="132,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__2" />
                                <Label Name="lbMap0x_3"
                                       Width="40"
                                       Height="25"
                                       Margin="173,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__3" />
                                <Label Name="lbMap0x_4"
                                       Width="40"
                                       Height="25"
                                       Margin="214,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__4" />
                                <Label Name="lbMap0x_5"
                                       Width="40"
                                       Height="25"
                                       Margin="255,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__5" />
                                <Label Name="lbMap0x_6"
                                       Width="40"
                                       Height="25"
                                       Margin="296,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__6" />
                                <Label Name="lbMap0x_7"
                                       Width="40"
                                       Height="25"
                                       Margin="338,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__7" />
                                <Label Name="lbMap0x_8"
                                       Width="40"
                                       Height="25"
                                       Margin="378,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__8" />
                                <Label Name="lbMap0x_9"
                                       Width="40"
                                       Height="25"
                                       Margin="419,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__9" />
                                <Label Name="lbMap0x_a"
                                       Width="40"
                                       Height="25"
                                       Margin="460,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__a" />
                                <Label Name="lbMap0x_b"
                                       Width="40"
                                       Height="25"
                                       Margin="501,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__b" />
                                <Label Name="lbMap0x_c"
                                       Width="40"
                                       Height="25"
                                       Margin="542,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__c" />
                                <Label Name="lbMap0x_d"
                                       Width="40"
                                       Height="25"
                                       Margin="583,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__d" />
                                <Label Name="lbMap0x_e"
                                       Width="40"
                                       Height="25"
                                       Margin="624,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__e" />
                                <Label Name="lbMap0x_f"
                                       Width="40"
                                       Height="25"
                                       Margin="665,0,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="__f" />

                                <Label Name="lbMap0x0_"
                                       Width="40"
                                       Height="25"
                                       Margin="0,20,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="0x0_" />
                                <TextBox Name="txtMap0x00"
                                         Width="40"
                                         Height="20"
                                         Margin="40,25,0,0"
                                         HorizontalAlignment="Left"
                                         VerticalAlignment="Top"
                                         HorizontalContentAlignment="Right"
                                         VerticalContentAlignment="Bottom"
                                         Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"
                                         FontFamily="Lucida Console"
                                         FontSize="12"
                                         GotFocus="txtMap_GotFocus"
                                         LostFocus="txtMap_LostFocus" />
                                <TextBox Name="txtMap0x01"
                                         Width="40"
                                         Height="20"
                                         Margin="81,25,0,0"
                                         HorizontalAlignment="Left"
                                         VerticalAlignment="Top"
